{
  "name": "@system76/standard",
  "version": "1.0.0",
  "description": "System76 standard JavaScript linting",
  "bin": "src/bin.js",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/system76/standard.git"
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "eslint": "^5.9.0",
    "standard-engine": "^10.0.0"
  },
  "devDependencies": {
    "ava": "^1.2.1",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "semantic-release": "^15.10.8"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "./src/bin.js",
    "release": "semantic-release",
    "test": "ava"
  },
  "publichConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "eslint",
    "hint",
    "lint",
    "regular",
    "standard",
    "style",
    "system76"
  ],
  "author": "System76 <hosting@system76.com> (https://system76.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/system76/standard/issues"
  },
  "homepage": "https://github.com/system76/standard#readme"
}
