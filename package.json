{
  "name": "@system76/standard",
  "version": "0.0.0-development",
  "description": "System76 standard JavaScript linting",
  "bin": "src/bin.js",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/system76/web-standard.git"
  },
  "dependencies": {
    "@system76/eslint-config-standard": "^1.0.2",
    "eslint": "~7.0.0",
    "standard-engine": "^10.0.0"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "semantic-release": "^15.13.12"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "./src/bin.js",
    "release": "semantic-release",
    "test": "ava"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "eslint",
    "hint",
    "lint",
    "regular",
    "standard",
    "style",
    "system76"
  ],
  "author": "System76 <hosting@system76.com> (https://system76.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/system76/web-standard/issues"
  },
  "homepage": "https://github.com/system76/web-standard#readme"
}
